# include <iostream.h>
# include <conio.h>

void main()
{
   int x, y, z, value;
   clrscr();
   cout << " Enter Any Value : ";
   cin >> value;
   x  =  0;  y  =  1;  z  =  0;
   cout << x << "\t" << y;
   z  =  x  +  y;
   while ( z   <=   value )
   {
      cout << "\t" << z;
      x  =  y;
      y  =  z;
      z  =  x  +  y;
   }
   getch();
}
